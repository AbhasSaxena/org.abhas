<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  <welcome-file-list>
  	
  	<welcome-file>SendingData.html</welcome-file>
  	
  	
  </welcome-file-list>
  
  <servlet>
  	<servlet-name>HelloServlet</servlet-name>
  	<display-name>HelloServlet</display-name>
  	<description>This  is my first servlet</description>
  	<servlet-class>trng.servlets.HelloServlet</servlet-class>
  	<load-on-startup>1</load-on-startup>
  </servlet>
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<display-name>LoginServlet</display-name>
  	<description></description>
  	<servlet-class>trng.servlets.LoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>HomeServlet</servlet-name>
  	<display-name>HomeServlet</display-name>
  	<description></description>
  	<servlet-class>trng.servlets.HomeServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Callingmethod</servlet-name>
  	<display-name>Callingmethod</display-name>
  	<description></description>
  	<servlet-class>trng.servlets.Caliingmethod</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ResponseServ</servlet-name>
  	<display-name>ResponseServ</display-name>
  	<description></description>
  	<servlet-class>trng.servlets.ResponseServ</servlet-class>
  </servlet>
  
  <servlet>
  	<servlet-name>CallingPost</servlet-name>
  	<display-name>CallingPost</display-name>
  	<description></description>
  	<servlet-class>trng.servlets.CallingPost</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>VisitorsCountServlet</servlet-name>
  	<display-name>VisitorsCountServlet</display-name>
  	<description></description>
  	<servlet-class>trng.servlets.VisitorsCountServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>HelloServlet</servlet-name>
  	<url-pattern>HelloServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/login.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>HomeServlet</servlet-name>
  	<url-pattern>/HomeServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Callingmethod</servlet-name>
  	<url-pattern>/Callingmethod</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ResponseServ</servlet-name>
  	<url-pattern>/ResponseServ</url-pattern>
  </servlet-mapping>
 
  <servlet-mapping>
  	<servlet-name>CallingPost</servlet-name>
  	<url-pattern>/CallingPost</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>VisitorsCountServlet</servlet-name>
  	<url-pattern>/VisitorsCountServlet</url-pattern>
  </servlet-mapping>
</web-app>
